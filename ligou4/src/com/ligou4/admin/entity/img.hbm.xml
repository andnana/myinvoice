<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ligou4.admin.entity.Img" 
		table="img">
		<id name="id" type="integer" 
			column="ID">
			<!-- 用来指明主键的生成方式 -->
			<generator class="native" />
				
		</id>
	
	<property name="imgname" type="java.lang.String" column="imgname" />
	<property name="suffix" type="java.lang.String" column="suffix" />
	<property name="colorname" type="java.lang.String" column="colorname" />
	<many-to-one name="products" column="productsid"  cascade="none" lazy="false" not-found="ignore"> 
		 
	</many-to-one>
	</class>
</hibernate-mapping>