<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.ligou4.entity.CommonInfo" 
		table="commoninfo">
		<id name="id" type="integer" 
			column="ID">
			<!-- 用来指明主键的生成方式 -->
			<generator class="native" />
				
		</id>
		<property name="introducetitle" 
			type="java.lang.String" column="introducetitle" />
	<property name="introducecontent" type="java.lang.String" column="introducecontent" />
	<property name="email" type="java.lang.String" column="email" />
	<property name="mobilephone" type="java.lang.String" column="mobilephone" />
	<property name="icpnumber" type="java.lang.String" column="icpnumber" />
	<property name="address" type="java.lang.String" column="address" />
	<property name="hotline" type="java.lang.String" column="hotline" />
	<property name="zipcode" type="java.lang.String" column="zipcode" />
	<property name="telephone" type="java.lang.String" column="telephone" />
		<property name="emailme" type="java.lang.String" column="emailme" />
		<property name="active" type="java.lang.Integer" column="active" />
	</class>
</hibernate-mapping>