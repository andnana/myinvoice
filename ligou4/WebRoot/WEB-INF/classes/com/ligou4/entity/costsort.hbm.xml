<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ligou4.entity.CostSort" 
		table="costsort">
		<id name="id" type="integer" 
			column="ID">
			<!-- 用来指明主键的生成方式 -->
			<generator class="native" />
				
		</id>
		<property name="begincost" 
			type="java.lang.Integer" column="begincost" />
		<property name="endcost" 
			type="java.lang.Integer" column="endcost" />
	</class>
</hibernate-mapping>