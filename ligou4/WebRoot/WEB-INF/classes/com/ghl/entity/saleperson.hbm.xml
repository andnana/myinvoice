<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ghl.entity.SalePerson" 
		table="saleperson">
		<id name="id" type="integer" 
			column="ID">
			<!-- 用来指明主键的生成方式 -->
			<generator class="native" />
				
		</id>
		<property name="salepersonname" 
			type="java.lang.String" column="salepersonname" />
		<property name="contact" 
			type="java.lang.String" column="contact" />
			<property name="address" 
			type="java.lang.String" column="address" />
	<property name="date" type="java.util.Date" column="date" />
	</class>
</hibernate-mapping>